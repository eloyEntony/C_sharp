<Window x:Class="MyAutoRun.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyAutoRun"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="488.821">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="356*"/>
            <RowDefinition Height="63*"/>
        </Grid.RowDefinitions>

        <ListBox ItemsSource="{Binding}"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="Add" Width="150" Height="30" Margin="5" Click="Add_Click"/>
            <Button Content="Delete" Width="150" Height="30" Margin="5" Click="Delete_Click"/>
        </StackPanel>
    </Grid>
</Window>
