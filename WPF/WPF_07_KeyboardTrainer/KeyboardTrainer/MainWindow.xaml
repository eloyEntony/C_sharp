                        <Border Background="#FFF07795" BorderBrush="Black" BorderThickness="2" CornerRadius="10" Margin="2">
                            <TextBlock Name="Oem3" FontSize="30" HorizontalAlignment="Center" Text="`" VerticalAlignment="Center"/>
